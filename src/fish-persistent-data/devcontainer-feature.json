{
  "name": "Fish - persistent data",
  "id": "fish-persistent-data",
  "version": "2.0.3",
  "description": "A feature that provides persistent data for Fish shell",
  "mounts": [
    {
      "source": "fish-persistent-data-${devcontainerId}",
      "target": "/mnt/fish-persistent-data",
      "type": "volume"
    }
  ],
  "onCreateCommand": [
    "/usr/local/share/github-nikobockerman/devcontainer-features/fish-persistent-data/onCreate.sh"
  ],
  "installsAfter": ["ghcr.io/devcontainers/features/common-utils"]
}
